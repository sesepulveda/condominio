
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/images/icono.png">
    <title>Adm Inmueble | Gestion Inmobiliaria</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/material/css/style.css" rel="stylesheet">
    <!-- You can change the theme colors from here -->
    <link href="~/material/css/colors/blue.css" id="theme" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="fix-header card-no-border logo-center">
    <div class="row" style=" padding-top:20px; ">
        <div class="col-md-2">
            &nbsp;
        </div>
        <div class="col-md-8">
            <div class="row">
                <div class="col-12">
                    <form class="form-horizontal form-material" id="loginform" action="../_layout.html">
                        <div class="card">
                            <div class="card-body">
                                <div class="text-center">
                                    <img alt="adm inmueble" src="~/assets/images/logoHorizontal.png" style=" max-width:500px" />
                                </div>
                                <div style=" padding-bottom:40px"></div>

                                <h1 class="card-title">Contactenos !</h1>
                                <h3 class="card-subtitle">Favor completar la siguiente infromacion ...</h3>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label><strong>Rut</strong></label>
                                            <input type="text" class="form-control form-control-line" required="" placeholder="Ej: 15.645.789-1">
                                        </div>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label><strong>Nombre completo</strong></label>
                                            <input type="text" class="form-control form-control-line" placeholder="Ej: Juan Carlos Perez Soto">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label><strong>Telefono</strong></label>
                                            <input type="text" class="form-control form-control-line" placeholder="Ej: +56 9 967 89 87 65">
                                        </div>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label for="example-email"><strong>Correo Electronico</strong> </label>
                                            <input type="email" id="example-email2" name="example-email" class="form-control" placeholder="Ej: contacto@adminmueble.cl">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label><strong>Seleccione opcion de contacto</strong></label>
                                            <select class="form-control">
                                                <option value="1">Soy Administrador</option>
                                                <option value="2">Soy copropietario</option>
                                                <option value="3">Otro</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label><strong>Detalle de Solicitud</strong></label>
                                    <textarea class="form-control" rows="5"></textarea>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 text-center">
                                        <button type="submit" class="btn btn-success" style=" width:100px">
                                            <i class=" fa fa-check"></i> Enviar
                                        </button>
                                        <button type="button" class="btn btn-inverse" id="button-back-contact" style=" width:100px">
                                            Volver
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="col-md-2">
            &nbsp;
        </div>
    </div>


    <script src="~/assets/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/assets/plugins/popper/popper.min.js"></script>
    <script src="~/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="~/material/js/jquery.slimscroll.js"></script>
    <!--Wave Effects -->
    <script src="~/material/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/js/sidebarmenu.js"></script>
    <!--stickey kit -->
    <script src="~/assets/plugins/sticky-kit-master/dist/sticky-kit.min.js"></script>
    <script src="~/assets/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!--Custom JavaScript -->
    <script src="~/material/js/custom.min.js"></script>
    <script src="~/material/js/jasny-bootstrap.js"></script>
    <!-- ============================================================== -->
    <!-- Style switcher -->
    <!-- ============================================================== -->
    <script src="~/assets/plugins/styleswitcher/jQuery.style.switcher.js"></script>
</body>
</html>
